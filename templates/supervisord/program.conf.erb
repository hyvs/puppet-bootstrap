[program:<%= @name %>]
command=<%= @command %>
user=<%= @user %>
autostart=true
autorestart=true
stdout_logfile=<%= @stdout_file %>
stderr_logfile=<%= @stderr_file %>
<%- if @numprocs -%>
numprocs=<%= @numprocs %>
process_name=%(program_name)s%(process_num)s
<%- end -%>
<%- if @numprocs_start -%>
numprocs_start=<%= @numprocs_start %>
<%- end -%>
<%- if @directory -%>
directory=<%= @directory %>
<%- end -%>